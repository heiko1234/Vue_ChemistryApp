

# creation with

npm create vue@latest



# frondend

cd vue_chemistry
npm install
npm run format
npm run dev



# virtual env

python3 -m venv .venv

# switch manually to virtual environment and then

source .venv/bin/activate

$(.venv) python3 -m pip install --upgrade pip

$(.venv) pip install poetry

$(.venv) poetry install

$(.venv) poetry update -lock


(Install: psycopg2)
apt-get update
apt-get install libpq-dev gcc


# backend fastapi

cd backend_service/

source .venv/bin/activate

(.venv) cd backend_service/backend_service/
(.venv) uvicorn main:app --reload


# check here locally: http://localhost:8000/docs



# activate python virtual environment

cd backend_service/
source .venv/bin/activate
python3



# This repo is to create frontend with a sync to backend. totally from scratch



